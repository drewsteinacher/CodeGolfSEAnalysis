         RowBox[{
         "Explicitly", " ", "left", " ", "empty", " ", "to", " ", "avoid", 
          " ", "pre"}], "-", 
         RowBox[{
         "loading", " ", "with", " ", "languages", " ", "that", " ", "never", 
          " ", "appear", " ", "on", " ", 
          RowBox[{"CodeGolf", ".", "SE"}]}]}], " ", "*)"}], 
       "\[IndentingNewLine]", 
       RowBox[{"\"\<Entities\>\"", "\[Rule]", 
        RowBox[{"<|", "|>"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"\"\<Properties\>\"", "\[Rule]", 
        RowBox[{"<|", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"\"\<Label\>\"", "\[Rule]", 
           RowBox[{"<|", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"\"\<Label\>\"", "\[Rule]", "\"\<label\>\""}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"\"\<DefaultFunction\>\"", "\[Rule]", 
              RowBox[{"Function", "[", 
               RowBox[{"e", ",", "\[IndentingNewLine]", 
                RowBox[{"Replace", "[", "\[IndentingNewLine]", 
                 RowBox[{
                  RowBox[{"Counts", "[", 
                   RowBox[{"SanitizeBadWords", "[", 
                    RowBox[{"e", "[", "\"\<Labels\>\"", "]"}], "]"}], "]"}], 
                  ",", "\[IndentingNewLine]", 
                  RowBox[{"{", "\[IndentingNewLine]", 
                   RowBox[{
                    RowBox[{
                    RowBox[{"<|", "|>"}], "\[RuleDelayed]", 
                    RowBox[{"Replace", "[", 
                    RowBox[{
                    RowBox[{"e", "[", "\"\<ProgrammingLanguage\>\"", "]"}], 
                    ",", 
                    RowBox[{"{", 
                    RowBox[{
                    RowBox[{"l_Entity", "\[RuleDelayed]", 
                    RowBox[{"l", "[", "\"\<Label\>\"", "]"}]}], ",", 
                    RowBox[{"_", "\[RuleDelayed]", 
                    RowBox[{"CanonicalName", "[", "e", "]"}]}]}], "}"}]}], 
                    "]"}]}], ",", "\[IndentingNewLine]", 
